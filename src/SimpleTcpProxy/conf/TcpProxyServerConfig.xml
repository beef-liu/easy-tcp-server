<?xml version="1.0" encoding="utf-8"?>
<TcpProxyServerConfig>
  <backendList>
    <BackendSetting>
      <tcpClientConfig>
        <connectTimeoutMS>3000</connectTimeoutMS>
        <host>127.0.0.1</host>
        <keepAlive>true</keepAlive>
        <port>6379</port>
        <receiveBufferSize>65536</receiveBufferSize>
        <reuseAddress>true</reuseAddress>
        <sendBufferSize>65536</sendBufferSize>
        <soTimeoutMS>1000</soTimeoutMS>
        <tcpNoDelay>true</tcpNoDelay>
      </tcpClientConfig>
    </BackendSetting>
  </backendList>
  <tcpServerConfig>
    <connectMaxCount>10000</connectMaxCount>
    <connectTimeout>1000</connectTimeout>
    <connectWaitCount>10000</connectWaitCount>
    <host>127.0.0.1</host>
    <port>16379</port>
    <readEventThreadCount>4</readEventThreadCount>
    <soTimeout>5000</soTimeout>
    <socketIOThreadCount>4</socketIOThreadCount>
    <socketReceiveBufferSize>65536</socketReceiveBufferSize>
    <socketSendBufferSize>65536</socketSendBufferSize>
    <tcpNoDelay>true</tcpNoDelay>
    <writeEventThreadCount>4</writeEventThreadCount>
  </tcpServerConfig>
</TcpProxyServerConfig>
